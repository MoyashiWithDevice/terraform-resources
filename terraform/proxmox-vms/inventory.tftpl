[proxmox_vms]
%{ for vm in vms ~}
%{ if try(flatten(vm.ipv4_addresses)[0], "") != "" ~}
${vm.name} ansible_host=${try(flatten(vm.ipv4_addresses)[0], "")} ansible_user=bababa
%{ endif ~}
%{ endfor ~}

[all:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
